{
	"name": "jaeger",
	"description": "Jaeger - Distributed Tracing",
	"version": "0.3.0",
	"publisher": "jaegertracing",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:jaeger.start",
		"onCommand:jaeger-OTLP.start",
		"onCommand:jaeger.ui",
		"onCommand:jaeger.stop"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/jaegertracing/jaeger-vscode.git"
	},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "jaeger.start",
				"title": "Start Jaeger",
				"category": "Observability"
			},
			{
				"command": "jaeger-OTLP.start",
				"title": "Start JaegerOTLP",
				"category": "Observability"
			},
			{
				"command": "jaeger.ui",
				"title": "Show Jaeger UI",
				"category": "Observability"
			},
			{
				"command": "jaeger.stop",
				"title": "Stop Jaeger",
				"category": "Observability"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -w -p ./"
	},
	"dependencies": {
		"@vscode/vsce": "^2.17.0"
	},
	"devDependencies": {
		"@types/node": "^10.5.2",
		"@types/vscode": "^1.38.0",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1"
	}
}
